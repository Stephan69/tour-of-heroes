{
  "name": "picamera",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "start-ir": "tsc && npm run copy-files && node dist/ir.js ir & echo $! > /tmp/.irpid &",
    "stop-ir": "kill $(cat /tmp/.irpid)",
    "start-express": "tsc && npm run copy-files && node dist/app.js app & echo $! > /tmp/.apppid &",
    "stop-express": "kill $(cat /tmp/.apppid)",
    "test": "echo \"Error: no test specified\" && exit 1",
    "copy-files": "cp -r src/data dist/"
  },
  "author": "sebastiaan",
  "license": "ISC",
  "dependencies": {
    "@types/helmet": "0.0.45",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "git": "^0.1.5",
    "helmet": "^3.21.3",
    "jimp": "^0.9.5",
    "lcd": "^3.0.0",
    "lcdi2c": "^1.0.6",
    "lirc-client": "^2.0.0",
    "lirc_node": "0.0.4",
    "nan": "^2.14.0",
    "native-node-utils": "^0.2.7",
    "node-gyp": "^6.1.0",
    "opencv4nodejs": "^5.5.0",
    "pi-camera-connect": "^0.3.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "description": ""
}
