{
  "compileOnSave": false,
  "compilerOptions": {
    "lib": ["es5", "es2015", "dom", "scripthost"],
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "ES2015",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES5",
  },
  "angularCompilerOptions": {
    "strictTemplates": true,
    "strictInjectionParameters": true
  }
}
